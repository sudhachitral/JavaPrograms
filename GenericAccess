import java.util.*;
class GenericAccess
{
    public static void main(String [] args)
    {
        ArrayList<Integer> a=new ArrayList<>();
        a.add(10);
        a.add(20);
        a.add(30);
        a.add(40);
        System.out.println(a);
        for(Integer ele:a)
        {
            System.out.println(ele);
        }
    }
}